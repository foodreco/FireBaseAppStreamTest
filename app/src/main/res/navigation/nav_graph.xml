<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_listFragment"
            app:destination="@id/listFragment" />
        <action
            android:id="@+id/action_mainFragment_to_onlyFragment"
            app:destination="@id/onlyFragment" />
        <action
            android:id="@+id/action_mainFragment_to_fireStoreListFragment"
            app:destination="@id/fireStoreListFragment" />
        <action
            android:id="@+id/action_mainFragment_to_FTSFragment"
            app:destination="@id/FTSFragment" />
        <action
            android:id="@+id/action_mainFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_mainFragment_to_storageFragment"
            app:destination="@id/storageFragment" />
        <action
            android:id="@+id/action_mainFragment_to_questionAndAnswerFragment"
            app:destination="@id/questionAndAnswerFragment" />
        <action
            android:id="@+id/action_mainFragment_to_analyticsFragment"
            app:destination="@id/analyticsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_fireStoreStatisticsFragment"
            app:destination="@id/fireStoreStatisticsFragment" />
    </fragment>
    <fragment
        android:id="@+id/listFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.list.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_listFragment_to_popUpFragment"
            app:destination="@id/popUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/onlyFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.list2.OnlyFragment"
        android:label="OnlyFragment"
        tools:layout="@layout/fragment_only" >
        <action
            android:id="@+id/action_onlyFragment_to_popUpFragment2"
            app:destination="@id/popUpFragment2" />
    </fragment>
    <fragment
        android:id="@+id/popUpFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.popup.PopUpFragment"
        android:label="fragment_pop_up"
        tools:layout="@layout/fragment_pop_up" >
        <argument
            android:name="diaryBase"
            app:argType="com.dreamreco.firebaseappstreamtest.room.entity.DiaryBase" />
    </fragment>
    <fragment
        android:id="@+id/popUpFragment2"
        android:name="com.dreamreco.firebaseappstreamtest.ui.popup.PopUpFragment2"
        android:label="PopUpFragment2"
        tools:layout="@layout/fragment_pop_up">
        <argument
            android:name="onlyBasic"
            app:argType="com.dreamreco.firebaseappstreamtest.room.entity.OnlyBasic" />
    </fragment>
    <fragment
        android:id="@+id/fireStoreListFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.firestorelist.FireStoreListFragment"
        android:label="fragment_fire_store_list"
        tools:layout="@layout/fragment_fire_store_list" />
    <fragment
        android:id="@+id/FTSFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.firestorefts.FTSFragment"
        android:label="FTSFragment"
        tools:layout="@layout/fragment_f_t"
        />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.dreamreco.firebaseappstreamtest.ui.login.ui.login.LoginFragment2"
        android:label="fragment_login2"
        tools:layout="@layout/fragment_login2" >
        <action
            android:id="@+id/action_loginFragment2_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/storageFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.fireStorage.StorageFragment"
        android:label="fragment_storage"
        tools:layout="@layout/fragment_storage" />
    <fragment
        android:id="@+id/questionAndAnswerFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.qna.QuestionAndAnswerFragment"
        android:label="fragment_question_and_answer"
        tools:layout="@layout/fragment_question_and_answer" >
        <action
            android:id="@+id/action_questionAndAnswerFragment_to_questionAndAnswerEditFragment"
            app:destination="@id/questionAndAnswerEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/questionAndAnswerWriteFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.qna.QuestionAndAnswerWriteFragment"
        android:label="QuestionAndAnswerWriteFragment"
        tools:layout="@layout/fragment_question_and_answer_write"/>
    <fragment
        android:id="@+id/questionAndAnswerEditFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.qna.QuestionAndAnswerEditFragment"
        android:label="QuestionAndAnswerEditFragment">
        <argument
            android:name="boardContent"
            app:argType="com.dreamreco.firebaseappstreamtest.repository.QuestionAndAnswerEditClass"/>
        <action
            android:id="@+id/action_questionAndAnswerEditFragment_to_questionAndAnswerFragment"
            app:destination="@id/questionAndAnswerFragment"
            app:popUpTo="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/analyticsFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.analytics.AnalyticsFragment"
        android:label="fragment_analytics"
        tools:layout="@layout/fragment_analytics" />
    <fragment
        android:id="@+id/fireStoreStatisticsFragment"
        android:name="com.dreamreco.firebaseappstreamtest.ui.fireStoreStatistics.FireStoreStatisticsFragment"
        android:label="fragment_fire_store_statistics"
        tools:layout="@layout/fragment_fire_store_statistics" />
</navigation>